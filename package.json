{
  "name": "@fil0157/ngx-themes",
  "version": "4.1.0",
  "description": "Angular module for creating themes in angular applications using native css variables",
  "author": "fil0157 <fil0157@ya.ru>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fil0157/ngx-themes.git"
  },
  "homepage": "https://ngx-themes.fil0157.ru",
  "bugs": {
    "url": "https://github.com/fil0157/ngx-themes/issues"
  },
  "scripts": {
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint ngx-themes",
    "lint:fix": "ng lint ngx-themes --fix",
    "e2e": "ng e2e",
    "ssr:dev": "ng run ngx-themes:serve-ssr",
    "ssr:serve": "node dist/app/server/main.js",
    "ssr:build": "ng build --prod && ng run ngx-themes:server:production",
    "prerender": "ng run ngx-themes:prerender",
    "lib:start": "ng build @fil0157/ngx-themes --watch",
    "lib:build": "ng build @fil0157/ngx-themes && node tools/build-lib.js",
    "lib:test": "ng test @fil0157/ngx-themes",
    "lib:test:run": "ng test @fil0157/ngx-themes --watch=false",
    "lib:size": "size-limit",
    "lib:lint": "ng lint @fil0157/ngx-themes",
    "lib:lint:fix": "ng lint @fil0157/ngx-themes --fix",
    "lib:publish": "npm run lib:build && cd dist/lib && npm pack && npm publish",
    "ngcc": "ngcc"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular-devkit/build-ng-packagr": "~0.1001.2",
    "@angular/animations": "^10.1.2",
    "@angular/cli": "~10.1.2",
    "@angular/common": "^10.1.2",
    "@angular/compiler": "^10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@angular/core": "^10.1.2",
    "@angular/forms": "^10.1.2",
    "@angular/platform-browser": "^10.1.2",
    "@angular/platform-browser-dynamic": "^10.1.2",
    "@angular/platform-server": "^10.1.2",
    "@angular/router": "^10.1.2",
    "@angular/service-worker": "^10.1.2",
    "@nguniversal/builders": "^10.1.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@size-limit/preset-small-lib": "^4.7.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.62",
    "angular-colorful": "^1.0.2",
    "codelyzer": "^6.0.0",
    "csscolorit": "^2.2.0",
    "express": "^4.17.1",
    "functionone": "^1.0.0",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "marked": "^1.1.1",
    "ng-packagr": "^10.1.0",
    "normalize.css": "^8.0.1",
    "protractor": "~7.0.0",
    "rxjs": "^6.6.3",
    "shelljs": "^0.8.4",
    "size-limit": "^4.7.0",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "typescript": "~3.9.7",
    "zone.js": "^0.10.3"
  },
  "keywords": [
    "angular",
    "themes",
    "ngx",
    "page theme",
    "variables"
  ]
}
