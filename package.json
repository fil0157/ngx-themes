{
  "name": "@fil0157/ngx-themes",
  "version": "3.1.0",
  "description": "Angular module for creating themes in angular applications using native css variables",
  "author": "fil0157",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fil0157/ngx-themes.git"
  },
  "homepage": "https://fil0157.github.io/ngx-themes",
  "scripts": {
    "start": "ng serve",
    "build": "ng build --prod",
    "test": "ng test",
    "lint": "ng lint ngx-themes",
    "lint:fix": "ng lint ngx-themes --fix",
    "release": "ng build --prod --output-path docs --base-href /ngx-themes/",
    "e2e": "ng e2e",
    "lib:start": "ng build @fil0157/ngx-themes --watch",
    "lib:build": "ng build @fil0157/ngx-themes --prod && node scripts/build-lib.js",
    "lib:test": "ng test @fil0157/ngx-themes",
    "lib:lint": "ng lint @fil0157/ngx-themes",
    "lib:lint:fix": "ng lint @fil0157/ngx-themes --fix",
    "lib:publish": "npm run lib:build && cd dist/lib && npm pack && npm publish",
    "ngcc": "ngcc",
    "dev:ssr": "ng run ngx-themes:serve-ssr",
    "serve:ssr": "node dist/ngx-themes/server/main.js",
    "build:ssr": "ng build --prod && ng run ngx-themes:server:production",
    "prerender": "ng run ngx-themes:prerender"
  },
  "dependencies": {},
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1001.2",
    "@angular-devkit/build-ng-packagr": "~0.1001.2",
    "@angular/animations": "^10.1.2",
    "@angular/cli": "~10.1.2",
    "@angular/common": "^10.1.2",
    "@angular/compiler": "^10.1.2",
    "@angular/compiler-cli": "~10.1.2",
    "@angular/core": "^10.1.2",
    "@angular/forms": "^10.1.2",
    "@angular/platform-browser": "^10.1.2",
    "@angular/platform-browser-dynamic": "^10.1.2",
    "@angular/platform-server": "^10.1.2",
    "@angular/router": "^10.1.2",
    "@nguniversal/builders": "^10.1.0",
    "@nguniversal/express-engine": "^10.1.0",
    "@types/express": "^4.17.0",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/node": "^12.12.62",
    "codelyzer": "^6.0.0",
    "express": "^4.17.1",
    "jasmine-core": "^3.6.0",
    "jasmine-spec-reporter": "^5.0.2",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "marked": "^1.1.1",
    "ng-packagr": "^10.1.0",
    "normalize.css": "^8.0.1",
    "protractor": "~7.0.0",
    "rxjs": "^6.6.3",
    "shelljs": "^0.8.4",
    "ts-node": "~8.3.0",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "typescript": "~3.9.7",
    "zone.js": "^0.10.3"
  },
  "keywords": [
    "angular",
    "angular themes",
    "ngx",
    "ngx-themes",
    "themes",
    "frontand",
    "typescript",
    "javascript",
    "js",
    "ts",
    "page theme",
    "module",
    "css",
    "variables",
    "castom theme"
  ],
  "main": "bundles/fil0157-ngx-themes.umd.js",
  "module": "fesm2015/fil0157-ngx-themes.js",
  "es2015": "fesm2015/fil0157-ngx-themes.js",
  "esm2015": "esm2015/fil0157-ngx-themes.js",
  "fesm2015": "fesm2015/fil0157-ngx-themes.js",
  "typings": "fil0157-ngx-themes.d.ts",
  "metadata": "fil0157-ngx-themes.metadata.json",
  "sideEffects": false
}
